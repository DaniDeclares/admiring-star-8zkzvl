{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "build"
}
